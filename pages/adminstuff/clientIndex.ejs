
<html lang="en">
  <%- include('partials/head') %>

  <head>
    <link rel="stylesheet" href="/css/admin/formstuff.css" />
  </head>

  <body>
    <%- include('partials/nav) %>
    <section id="select">
      <div class="container-md">
        <div class="row gx-5">
          <div class="col py-2">
            <h3 class="border-bottom text-center py-2">Select the operation</h3>
            <div class="py-3"></div>
            <!-- select what u want here -->

            <div
              class="btn-group-vertical ps-3 d-flex justify-content-center"
              role="group"
            >
              <input
                type="radio"
                class="btn-check"
                name="vbtn-radio"
                id="vbtn-radio1"
                onclick="addClient() "
              />
              <label class="btn btn-outline-danger" for="vbtn-radio1"
                >Add Client</label
              >

              <input
                type="radio"
                class="btn-check"
                name="vbtn-radio"
                id="vbtn-radio3"
                onclick="removeClient()"
              />
              <label class="btn btn-outline-danger" for="vbtn-radio3"
                >Remove Client</label
              >
            </div>
          </div>
          

          <div class="col py-5">
            <!-- iframe here -->
            <div class="py-3"></div>
            <iframe frameborder="2" id="frame" title="myframe"></iframe>
          </div>
        </div>
      </div>
    </section>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
    <script src="/js/admin_js/editpackage.js"></script>
    <script src="/js/admin_js/editclient.js"></script>
  </body>
</html>
