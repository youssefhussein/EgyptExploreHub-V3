<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin Panel</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="/css/general.css" />

    <link rel="stylesheet" href="/css/admin.css" />
    <!-- chart js script -->
    <!--This page is to be loaded after successful admin login from @FatmaNageh-->
<link rel="stylesheet" href="/css/formstuff.css">
<script async src="/js/admin_js/editpackage.js"></script>
  </head>

  <body>
    <%- include('partials/nav') %>
    <section id="select">
      <div class="container-md">
        <div class="row gx-5 ">
          <div class="col  py-5">
            <h3 class="border-bottom text-center py-2">Select the operation</h3>
            <div class="py-3"></div>
            <!-- select what u want here -->
           
            <div class="btn-group-vertical ps-3 d-flex justify-content-center" role="group">
              <input type="radio" class="btn-check" name="vbtn-radio" id="vbtn-radio1"  onclick="addPKG() ">
              <label class="btn btn-outline-danger" for="vbtn-radio1">Add Package</label>
              <input type="radio" class="btn-check" name="vbtn-radio" id="vbtn-radio2"  onclick="editPKG()">
              <label class="btn btn-outline-danger" for="vbtn-radio2">Edit Package</label>
              <input type="radio" class="btn-check" name="vbtn-radio" id="vbtn-radio3"  onclick="removePKG()">
              <label class="btn btn-outline-danger" for="vbtn-radio3">Remove Package</label>
            </div>














          </div>

          <div class="col  py-5">
            <!-- iframe here -->
<div class="py-3"></div>
<iframe frameborder="2" id="frame"  title="myframe"></iframe>


          </div>
        </div>
      </div>
    </section>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
