<!DOCTYPE html>
<html lang="en">
  <%- include('../../partials/head') %>
<body   class="bg-gray">
    
<% packs.forEach(element => { %>
 <div class="container">
<div class="row">
  <div class="col-md-4">
      <h3><%= element.name %></h3>
      <p><%= element.description %></p>
      <p>Location: <%= element.location %></p>
      <p>Price: <%= element.price %></p>
      <p>Start Date: <%= element.startAt %></p>
      <p>End Date: <%= element.endAt %></p>
  </div>
</div>




 </div>




<% }) %>




</body>
</html>